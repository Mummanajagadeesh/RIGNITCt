{
  "name": "rignitc",
  "version": "1.0.0",
  "description": "Website project.",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf dist",
    "copy:root": "cpx \"{404.html,CNAME,robots.txt,sitemap.xml,index.html}\" dist",
    "copy:assets": "cpx \"assets/**/*\" dist/assets",
    "copy:pages": "cpx \"src/pages/**/*\" dist/src/pages",
    "copy:styles": "cpx \"src/styles/**/*\" dist/src/styles",
    "copy:scripts": "cpx \"src/scripts/**/*\" dist/src/scripts",
    "copy:components": "cpx \"src/components/**/*\" dist/src/components",
    "copy:data": "cpx \"src/data/**/*\" dist/src/data",
    "build": "npm run clean && npm run copy:root && npm run copy:assets && npm run copy:pages && npm run copy:styles && npm run copy:scripts && npm run copy:components && npm run copy:data",
    "postbuild": "node utils/clean_urls.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "cpx": "^1.5.0"
  }
}